<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AlertManager.Views.EventListWindowView"
        Title="EventListWindowView">



	<Grid RowDefinitions="*, Auto">
		<Grid Grid.Row ="0">
			<UserControl Content="{Binding EventListUserControl}">

			</UserControl>
		</Grid >

		<Grid Grid.Row ="1" HorizontalAlignment="Right" Height="30" Margin="0, 15, 0, 0">
			<StackPanel Orientation="Horizontal" Spacing="15">
				<Button Content="Close" Name ="closeWindow"></Button>
			</StackPanel>
		</Grid>
	</Grid>
</Window>
