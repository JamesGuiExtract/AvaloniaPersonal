
(?#
#--------------------------------------------
# "LENDER IS" or "C" followed by "LENDER"
#--------------------------------------------
)

(?:

	(?#
	#--------------------------------------------
	# "LENDER IS" near the beginning of a line
	#--------------------------------------------
	)	

	^[\s\S]{0,10}(?:LENDE|NDER)[\s\S]{0,10}?\b[I1!|][5$S]
	
	| (?# OR...)
	
	(?#
	#--------------------------------------------
	# "C" followed by "LENDER"
	#--------------------------------------------
	)	

	^[\s\S]{0,5}\(\s?C\s?\)[\s\S]{0,5}(?:LEND|NDER)

)

(?#
#------------------------------------------------
# Grab the Beneficiary, which can be up to 200
# characters long
#------------------------------------------------
)

(?<BENEFICIARY>[\s\S]{5,200}?)

(?#
#------------------------------------------------
# We can tell where the beneficiary name ends by
# looking for the clues "LENDER IS", "LENDER'S 
# ADDRESS", or "SINGLE FAMILY"
#------------------------------------------------
)

(?:

	(?#
	#------------------------------------------------
	# "LENDER" followed by... 
	#------------------------------------------------
	)
			
	(?:LENDE|NDER)
			
	(?: 
		(?#
		#------------------------------------------------
		# ADDRESS
		# NRS Example: 0001.tif
		#------------------------------------------------
		)
		
		[\s\S]{0,12}?(?:ADDR|DRESS)
					
		| (?# OR...)
		
		(?#
		#------------------------------------------------
		# IS
		# NRS Example: 0002.tif
		#------------------------------------------------
		)
		
		[\s\S]{0,9}?[I1!|]\s?[5$S]

	)
				
	|(?# OR... )

	(?#
	#------------------------------------------------
	# SINGLE FAMILY
	# NRS Example: 0003.tif
	#------------------------------------------------
	)

	(?:SING|NGLE)[\s\S]{0,13}(?:FAMI|MILY)
	
	|(?# OR... )
	
	(?#
	#------------------------------------------------
	# CALLED
	#------------------------------------------------
	)
	
	\bCALLED\b

)