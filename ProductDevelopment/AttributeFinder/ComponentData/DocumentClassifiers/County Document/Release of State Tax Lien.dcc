	OR_BEGIN,ROSTL_PROBABLE_01,TRUE
		01=\b[\s\S]{0,15}?STATE\s*TAX\s*LIEN\s*RELEASE\b
	OR_END
SCOPE_END
SCOPE_BEGIN,0,0.4,1,1
	OR_BEGIN,ROSTL_PROBABLE_02,FALSE
		01=^RELEASE\s*OF\s*LIEN\s*OFFICE\s*OF\s*THE\s*TAX\s*COLLECTOR(?:\r?$)
	OR_END
SCOPE_END

[PROBABLE_END]


