		//
		// Power of Attorney
		// 0002.tif;K:\Common\Engineering\Sample Files\ACS\TX - Midland\Set001\Images\POWER OF ATTORNY\00016270.tif.uss
		//
		// Statutory Durable Power of Attorney
		// 0003.tif;K:\Common\Engineering\Sample Files\ACS\TX - Midland\Set001\Images\POWER OF ATTORNY\00016409.tif.uss
		//
		// General Power of Attorney
		// 0004.tif;K:\Common\Engineering\Sample Files\ACS\TX - Midland\Set001\Images\POWER OF ATTORNY\00016590.tif.uss
		//
