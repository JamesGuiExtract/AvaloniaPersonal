	OR_END
SCOPE_END

SCOPE_BEGIN,0,0.5,1,1
	OR_BEGIN,WD_PROBABLE_02,false
		// example where pattern exists, but requires more chars between "made" and "between"
		// 0029.tif;K:\Common\Engineering\Sample files\ACS\PA - Allegheny\Set002\Images\D\00015954.tif.uss
		// NOTE: "300" was not used in the sure level rule because of the risk of more matches in
		// non-mortgage documents
		// "made" not OCR'ed - so left it optional
		// 0030.tif;K:\Common\Engineering\Sample files\ACS\PA - Allegheny\Set002\Images\D\00015796.tif.uss
		// example that contains "T H I S  I N D E N T U R E"
		// 0031.tif;K:\Common\Engineering\Sample files\ACS\PA - Allegheny\Set002\Images\D\00017686.tif.uss
		// example that contains "T H I S" and "I N D E N T U R E" on seperate lines
		// 0032.tif;K:\Common\Engineering\Sample files\ACS\PA - Allegheny\Set002\Images\D\00017179.tif.uss
		// example that does not contain "this", but rest of the pattern is the same.
		// 0033.tif;K:\Common\Engineering\Sample files\ACS\PA - Allegheny\Set002\Images\D\00015857.tif.uss
		01=\bT\s*h\s*i\s*s\s*(?:D\s*e\s*e\s*d(?!\s*OF\s*TRUST)|I\s*n\s*d\s*e\s*n\s*t\s*u\s*r\s*e)[\s\S]{0,40}?(?:Made)?[\s\S]{1,300}?(?:Between|by)

		//0034.tif;K:\Common\Engineering\Sample Files\ACS\PA - Allegheny\Set002\Images\D\00018913.tif.uss
		02=^GENERAL[\s\S]{0,2}?DEED(?:\r?$)

		// example where this "deed" or "indenture" did not get OCR'ed properly but the words following
		// 0035.tif;K:\Common\Engineering\Sample files\ACS\PA - Allegheny\Set002\Images\D\00018202.tif.uss
		// 	may have sufficient clues that this is a warranty deed
		// example where the dollar amount is just $1 - so there's only 1 digit after the $
		// 0036.tif;K:\Common\Engineering\Sample files\ACS\PA - Allegheny\Set002\Images\D\00017413.tif.uss
		// example that has a lot (about 370 chars) of chars
		// 0037.tif;K:\Common\Engineering\Sample files\ACS\PA - Allegheny\Set002\Images\D\00017949.tif.uss
		// 	in between  "between" and "witnesseth"
		03=\bMADE[\s\S]{1,50}?BETWEEN[\s\S]{20,400}?WITNESSETH[\s\S]{20,200}?\$[0-9\.,]{1,}
		
		// CORPORATION GRANT .EED
		// 0054.tif;k:\common\engineering\sample files\atpac\ca - contracosta\set002\images\grant deed\20059023088300.tif.uss
		04=^(?:(?:CORPORATION|INDIVIDUAL|CALIFORNIA)\s*)?GRANT\s*.EED(?:\r?$)
	OR_END

	OR_BEGIN,WD_PROBABLE_03,TRUE
		// 0018.tif;K:\Common\Engineering\Sample Files\ACS\TX - Midland\Set001\Images\WARRANTY DEED\00016233.tif.uss
