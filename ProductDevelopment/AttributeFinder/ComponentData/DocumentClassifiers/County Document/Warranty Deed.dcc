#import "CommonDT.dcc"

// **********
// Zero Block
// **********

[ZERO_BEGIN]

SCOPE_BEGIN,0,1,1,1
	OR_BEGIN,WD_ZERO_01,false
		// if "quit claim deed" is on its own line, then the document is not a warranty deed
		// K:\Common\Engineering\Sample files\ACS\PA - Allegheny\Set002\Images\D\00017765.tif.uss
		//is an example of a warranty deed that contains the words "quit claim deed"
		// embedded in a paragraph of text. 
		//K:\Common\Engineering\Sample files\ACS\PA - Allegheny\Set002\Images\D\00016626.tif.uss
		//also contains "quit claim deed" in paragraph text
		01=^QUIT-?\s*CLAIM\s*DEED(?:\r?$)

		02=\bTHIS\s*QUIT-?\s*CLAIM\s*DEED\b

		//K:\Common\Engineering\Sample Files\ACS\PA - Allegheny\Set002\Images\D\00018324.tif.uss
		03=\bquit-?\s*claim\(?s?\)?\s*(?:un)?to\b
		04=\bEXHIBIT\s*A\s*TO\s*WARRANTY\s*DEED\b
		05=^\s*REVOLVING\s*CREDIT\s*DEED\s*OF\s*TRUST\s*(?:\r?$)
		
		// Because of their similarity in wording, Sheriff's Deeds will often classify
		// as Warranty Deeds.
		//K:\Common\Engineering\Sample Files\ACS\MI - Washtenaw\Set008\Images\DEED SHERIFF CLK-JUD\B06174AA.AGP.TIF
		06=^SHERIFF.?S\s*DEED\s*ON\s*MORTGAGE\s*(?:SALE|FORECLOSURE)
	OR_END
SCOPE_END 

SCOPE_BEGIN,0,0.1,1,1
	SINGLE_BEGIN,WD_ZERO_03,false
		//K:\Common\Engineering\Sample Files\ACS\TX - Midland\Set003\Images\RELEASE OF LIEN\00012260.tif.uss 
		^Release\s*of\s*L(?:ie|ei)ns?(?:\r?$)
	SINGLE_END
SCOPE_END 
SCOPE_BEGIN,0,0.5,1,2
	OR_BEGIN,WD_ZERO_04,false
	//I:\Common\Engineering\ProductDevelopment\AttributeFinder\NationalRuleSet\County\Images\GrantorGrantee\Quit Claim Deed\Grantor\0005.tif.uss
	01=^DEED\s*OF\s*CONVEYANCE[\s\S]{0,5}\(Quit\s*Claim\s*Deed\)
	02=^.{0,3}Collateral\s*Assignment\s*of
	OR_END
SCOPE_END 
[ZERO_END]


// **********
// Sure Block
// **********
[SURE_BEGIN]

SCOPE_BEGIN,0,1,1,2
	OR_BEGIN,WD_SURE_01,false
		// WARRANTY DEED
		// 0001.tif;K:\Common\Engineering\Sample files\Fort Bend Central Appraisal District - TX\Set001\IMAGES\DEED\2003133416.tif.uss
		// General warranty Deed
		// 0002.tif;K:\Common\Engineering\Sample files\Fort Bend Central Appraisal District - TX\Set001\IMAGES\DEED\2003132633.tif.uss
		// GENERAL WARRANTY DEED WITH VENDOR'S LIEN
		// 0003.tif;K:\Common\Engineering\Sample files\Fort Bend Central Appraisal District - TX\Set001\IMAGES\DEED\2003133420.tif.uss
		// SPECIAL WARRANTY DEED
		// 0004.tif;K:\Common\Engineering\Sample files\Fort Bend Central Appraisal District - TX\Set001\IMAGES\DEED\2003133404.tif.uss
		// CORPORATE WARRANTY DEED
		// 0005.tif;K:\Common\Engineering\Sample files\Fort Bend Central Appraisal District - TX\Set001\IMAGES\DEED\2003134124.tif.uss
		// CONDOMINIUM DEED
		// 0014.tif;K:\Common\Engineering\Sample files\Waukesha\Set003\Images\Warranty Deed\002940.tif.uss
		// FIDUCIARY DEED
		// MINERAL WARRANTY DEED
		// 0016.tif;K:\Common\Engineering\Sample Files\ACS\TX - Midland\Set004\Images\WARRANTY DEED\00015098.tif.uss
		// OWELTY DEED
		// 0006.tif;K:\Common\Engineering\Sample Files\Fort Bend Central Appraisal District - TX\Set002\Images\DEED\2003131634.tif.uss
		// GENERAL WARRANTY DEED - GIFT
		// 0007.tif;K:\Common\Engineering\Sample Files\Fort Bend Central Appraisal District - TX\Set006\Images\DEED\2003141439.tif.uss
		//
		01=^(?:(?:SUBSTITUTED)?(?:SPECIAL|CASH|MINERAL|GENERAL|ASSUMPTION|CORPORATE|CORRECTION)\s*)?(?:WARRANTY|CONDOMINIUM|FIDUCIARY|OWELTY)\s*DEED\s*(?:-\s*[\s\S]{1,15})?(?:\s*(?:OF|-)\s*GIFT)?(?:\r?$)

		// DEED WITH VENDOR'S LIEN
		//
		// GENERAL WARRANTY DEED WITH VENDORS LIEN
		// 0017.tif;K:\Common\Engineering\Sample Files\ACS\TX - Midland\Set001\Images\WARRANTY DEED\00016348.tif.uss
		02=^(?:(?:GENERAL|SPECIAL)\s*)?(?:WARRANTY\s*)?DEED[\s\S]{0,15}WITH\s*VENDOR[\s\S]{0,5}LIEN(?:(?:-\s*)?[\s\S]{1,15})?

		//
		03=^WARRANTY\s*DEED\s*IN\s*LIEU\s*OF\s*FORECLOSURE\s*(?:\r?$)

		// Deed under Order of Sale in Tax Suits
		// 0008.tif;K:\Common\Engineering\Sample Files\Fort Bend Central Appraisal District - TX\Set003\Images\DEED\2003134416.tif.uss
		04=^DEED\s*UNDER\s*ORDER\s*OF\s*SALE\s*IN\s*TAX\s*SUITS(?:\r?$)

		// GRANT DEED
		// 0021.tif;K:\Common\Engineering\Sample Files\AtPac\CA - Fresno\Set003\Images\Deed\20049000000400.002.tif.uss
		// INDIVIDUAL GRANT DEED
		// 0022.tif;K:\Common\Engineering\Sample Files\AtPac\CA - Fresno\Set003\Images\Deed\20049000001300.001.tif.uss
		// CORPORATION GRANT DEED
		// 0023.tif;K:\Common\Engineering\Sample Files\AtPac\CA - Fresno\Set003\Images\Deed\20049000118700.003.tif.uss
		05=^(?:(?:CORPORATION|INDIVIDUAL|CALIFORNIA)\s*)?GRANT\s*DEED(?:\r?$)

		// INTERSPOUSAL TRANSFER DEED
		// 0024.tif;K:\Common\Engineering\Sample Files\AtPac\CA - Fresno\Set003\Images\Deed\20049000090300.002.tif.uss
		// INTERSPOUSAL TRANSFER GRANT DEED
		// 0025.tif;K:\Common\Engineering\Sample Files\AtPac\CA - Fresno\Set003\Images\Deed\20049000001000.002.tif.uss
		06=^(?:INTERSPOUSAL|TRUST)\s*TRANSFER\s*(?:GRANT\s*)?DEED(?:\r?$)

		// SUBSTITUTE TRUSTEE'S DEED
		// 0009.tif;K:\Common\Engineering\Sample Files\Fort Bend Central Appraisal District - TX\Set006\Images\DEED\2003142079.tif.uss
		// SUBSTITUTE TRUSTEE'S FORECLOSURE DEED
		// 0010.tif;K:\Common\Engineering\Sample Files\Fort Bend Central Appraisal District - TX\Set003\Images\DEED\2003134572.tif.uss
		07=^SUBSTITUTE\s*TRUSTEE.?S\s*(?:FORECLOSURE\s*)?DEED(?:\r?$)

		// EXECUTOR'S MINERAL AND ROYALTY DEED
		// 0011.tif;K:\Common\Engineering\Sample Files\Fort Bend Central Appraisal District - TX\Set003\Images\DEED\2003134820.tif.uss
		// TRUSTEE'S MINERAL AND ROYALTY DEED
		// 0012.tif;K:\Common\Engineering\Sample Files\Fort Bend Central Appraisal District - TX\Set003\Images\DEED\2003134816.tif.uss
		// MINERAL AND ROYALTY DEED TO TRUSTEE
		// 0013.tif;K:\Common\Engineering\Sample Files\Fort Bend Central Appraisal District - TX\Set004\Images\DEED\2003135879.tif.uss
		08=^(?:(?:TRUSTEE|EXECUTOR).?S\s*)?MINERAL\s*AND\s*ROYALTY\s*DEED(?:\s*TO\s*TRUSTEE)?(?:\r?$)

		09=^NOTICE\s*OF\s*CONFIDENTIALITY\s*RIGHTS[\s\S]{0,500}WARRANTY\s*DEED\b
		
		10=\bWARRANTY\s*DEED[\s\S]{0,300}\r\nNOTICE\s*OF\s*CONFIDENTIALITY\s*RIGHTS\b

		11=^LIMITED\s*SPECIAL\s*WARRANTY\s*DISTRIBUTION\s*DEED\s*(?:\r?$)
		
		// RJB 060103
		// 0026.tif;K:\Common\Engineering\Sample Files\The_Fund\FL - Sarasota\Set002\Images\Multi-Page\D\2005272589.tif.uss
		12=^PERSONAL\s*REPRESENTATIVE.?S\s*DEED\s*(?:\r?$)
		// K:\Common\Engineering\Sample Files\AtPac\OR - Washington\Set001\Images\BARGAIN and SALE DEED\2005-025026.tif.uss
		13=^(?:STATUTORY\s*)?BARGAIN\s*AND\s*SALE\s*DEED(?:\r?$)
		// K:\Common\Engineering\Sample Files\AtPac\OR - Washington\Set001\Images\BARGAIN and SALE DEED\2005-026902.tif.uss
		14=^BARGAIN\s*AND\s*SALE\s*DEED\s*\-?\s*STATUTORY\s*\-?\s*FORM(?:\r?$)


	OR_END
SCOPE_END
SCOPE_BEGIN,0,0.6,1,2
	OR_BEGIN,WD_SURE_02,false
//		0054.tif;K:\Common\Engineering\Sample Files\DTS\NV - Clark\Set001\Images\Deed\200406290001062.tif.uss
		01=^Grant[^\w\r\n]*Bargain[^\w\r\n]*(?:and[^\w\r\n]*)?Sale[^\w\r\n]*Deed\r\n
	OR_END
SCOPE_END

[SURE_END]

