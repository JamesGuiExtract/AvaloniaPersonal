//	address shown below: ... \d{5)


// $1 is the PREFIX
(
\[?(?::?
 (?:
	// The "\|" and "I" at the end is for ocr errors on / no template exists
	(?:(?:(?:(?:(?:this\s+document\s+was\s+)?prepared)|(?:recording\s+requested))\s+by\s*)?(?:and|&|/|I|\|)\s*)?	
	(?:(?:
       (?:when\s+recorded)|
	 (?:after\s+recording(?:\s+of\s+this\s+document)?)|
	 (?:after\s+recorded)|
	 (?:record\s+(?:and|&))|
	 (?:recorder)
	),?\s+)?
	(?:(?:please\s+)?(?:mail|retu(?:rn|m))\s+(?:this\s+)?(?:(?:(?:original\s+)|(?:recorded\s+))?(?:document|instrument|reconveyance)\s+)?(?:by\s+mail\s+)?to)
	(?:\s+mortgagee\s+at)?
	(?:\s+address\s+shown\s+below)?
	(?:\s+and\s+prepared\s+by)?
 )
 |
 (?:(?:(?:name|record)\s+(?:and|&)\s+)?retu(?:rn|m)\s+address)
 |
 (?:mail\s+recorded\s+satisfaction\s+to)
 |
 (?:recorder\s+of\s+deeds\s+is\s+requested\s+to\s+mail
  \s+this\s+satisfaction\s+piece\s+to)
 |
 (?:
  (?:recorder\s+of\s+deeds\s+is\s+requested\s+to\s+)?
  mail\s+this\s+(?:assignment|satisfaction)\s+(?:peice\s+)?to
 )
 |
 (?:this\s+instrument\s+prepared\s+by\s+and\s+to\s+be\s+returned\s+to)
 |
 (?:record\s+(?:and|&)\s+return)
 |
 (?:after\s+recordation\s+this\s+mortgage\s+should\s+be\s+returned\s+to)
)\]?
// The comma, semi-colon, "*" and hyphen are to account for ocr errors
// they are not in any document template
(?:\s*[:.,-;*]\s*)?
(?:\s*\(?:name,?\s*address,?\s*city,?\s+and\s+state\))?
){1,}

// $2 is the BODY
(
// Body of Address
[\s\S]{1,200}?\b\s*
)

// $3 is the state
// State Part of Address
(
	ALABAMA|AL|
	ALASKA|AK|
	AMERICAN\s+SAMOA|AS|
	ARIZONA|AZ|
