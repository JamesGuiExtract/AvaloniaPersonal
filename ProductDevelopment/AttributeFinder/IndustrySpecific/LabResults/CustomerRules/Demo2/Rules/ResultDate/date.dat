(?x)(
  \d{1,2}/\d{1,2}/\d{2}
    (?:\d{2}|[5S](?=.{0,20}[\r\n]+\d{2}:\d{2}))?
    (?:[-\s]?\d{1,2}(?:\s?:)?\s?\d{2}(?:\s?:00)?|-UNK)?
    (?:\s*[AP]M\b)?
|
  (?:Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec)
    \s\d{1,2},?\s\d{4}
    (?:\s|@)
    \d{1,2}:\d{2}
    (?:\s*[AP]M\b)?
|
  (?<=\s|^)
  (
      1(?=[0-2])
    |
      0(?=[1-9])
  )
    \d
    -
  (
      0(?=[1-9])
    |
      [1-2](?=[0-9])
    |
      3(?=[01])
  )
    \d
  (?=\s|$)
  (\s\d{4})?
)
