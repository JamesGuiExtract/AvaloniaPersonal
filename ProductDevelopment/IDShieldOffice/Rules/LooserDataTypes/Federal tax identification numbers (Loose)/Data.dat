(?imnx) (?# Case insensitive, multiline, explicit capture, ignore pattern whitespace)

(?#
#------------------------------------------------
# Numbers with the form ##-#######
#------------------------------------------------
)

(?:

	(?#
	#--------------------------------------------
	# The number does not start with a digit or a
	# dash
	#--------------------------------------------
	)
	
	(?:^|[^\d-])

	(?-i:(?<TaxID_Data>[\dOolI!|BS$]{2}\x20*-\x20*[\dOolI!|BS$]{7}))
	
	(?#
	#--------------------------------------------
	# The number ends with a non-digit or a new 
	# line
	#--------------------------------------------
	)
	
	(?:[^\d-]|$)
	
)

|(?# Or...)

(?#
#------------------------------------------------
# A Tax ID clue...
#------------------------------------------------
)

(?:

	(?<!p(roperty|arcel).{0,4})
	
	(?:
		
		(?#
		#----------------------------------------
		# Various forms of "Tax Identification 
		# Number"
		#----------------------------------------
		)
		
		(?:
		
			(?#
			#------------------------------------
			# "Tax", "Taxpayer", or "Employer"...
			#------------------------------------
			)
			(?:federal\s)?(?:(?:\bta)?x(?:payer)?\b|employer)
			
			(?#
			#------------------------------------
			# ...followed by some form of "ID" or
			# identification...
			#------------------------------------
			)
			\D{0,4}(?:i\s?[,.]?\s?d\s?[,.]?|ident[\s\S]{0,11}|[\s\S]{1,6}ification)
			
			(?#
			#------------------------------------
			# ...followed by some form of 
			# "Number", "No", a pound sign, or a
			# colon...
			#------------------------------------
			)
			
			[\s\S]{0,7}?(?:NUM(?:BERS?)?|N[O0]\b|\#|[:;])
	 
		)

		|(?# Or...)
	
		(?#
		#----------------------------------------
		# Various forms of "TIN", "EIN", and 
		# "FEIN"
		# This expression is CASE SENSITIVE!
		#----------------------------------------
		)
		
		(?:\b(?-i:(?:[TE]|F\s?[,.]?\s?E)\s?[,.]?\s?[Iil1]\s?[,.]?\s?(?:N\b|\#)))
	
	)
	
	(?#
	#--------------------------------------------
	# Shortly followed by nine digits
	#--------------------------------------------
	)
	
	[\s\S]{0,7}?(?<TaxID_Data>\b(?-i:[\dOolI!|BS$]{9}))(?:[^\d-]|$)
	
)