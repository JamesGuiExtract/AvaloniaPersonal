<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <OutputPath>..\..\..\..\..\Binaries\$(Configuration)\</OutputPath>
    <Tailcalls>true</Tailcalls> <!--Do tail call elimination even in debug config-->
    <Optimize>false</Optimize> <!-- Optimizing caused odd errors when this assembly is referenced from an F# Preprocessor script-->
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\..\..\Core\Code\ExtractInternalKey.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="DTO.fs" />
    <Compile Include="Utils.fs" />
    <Compile Include="NamedPipe.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" />
    <PackageReference Include="FsPickler" Version="5.3.2" />
  </ItemGroup>

</Project>
