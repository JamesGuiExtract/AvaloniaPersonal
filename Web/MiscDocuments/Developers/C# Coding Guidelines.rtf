\par }{\f2\fs20\lang1024\langfe1024\noproof \{
\par     }{\f2\fs20\cf11\lang1024\langfe1024\noproof // Implement IDisposable.
\par }{\f2\fs20\lang1024\langfe1024\noproof     }{\f2\fs20\cf2\lang1024\langfe1024\noproof public}{\f2\fs20\lang1024\langfe1024\noproof  }{\f2\fs20\cf2\lang1024\langfe1024\noproof void}{\f2\fs20\lang1024\langfe1024\noproof  Dispose()
\par     \{
\par         Dispose(}{\f2\fs20\cf2\lang1024\langfe1024\noproof true}{\f2\fs20\lang1024\langfe1024\noproof );
\par         }{\f2\fs20\cf17\lang1024\langfe1024\noproof GC}{\f2\fs20\lang1024\langfe1024\noproof .SuppressFinalize(}{\f2\fs20\cf2\lang1024\langfe1024\noproof this}{\f2\fs20\lang1024\langfe1024\noproof );
\par     \}
\par 
\par     }{\f2\fs20\cf2\lang1024\langfe1024\noproof protected}{\f2\fs20\lang1024\langfe1024\noproof  }{\f2\fs20\cf2\lang1024\langfe1024\noproof virtual}{\f2\fs20\lang1024\langfe1024\noproof  }{\f2\fs20\cf2\lang1024\langfe1024\noproof void}{
\f2\fs20\lang1024\langfe1024\noproof  Dispose(}{\f2\fs20\cf2\lang1024\langfe1024\noproof bool}{\f2\fs20\lang1024\langfe1024\noproof  disposing)
